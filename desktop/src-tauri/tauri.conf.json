{
  "tauri": {
    "bundle": {
      "active": true,
      "targets": ["deb", "msi", "appimage", "dmg"],
      "identifier": "com.lights0123.n-link",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Copyright (c) 2021 Ben Schattinger. Licensed under GPL-3.0",
      "category": "Utility",
      "shortDescription": "Free, cross-platform, CX-II compatible computer linking program for the TI-Nspire",
      "longDescription": "Free, cross-platform, CX-II compatible computer linking program for the TI-Nspire",
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "shell": {
        "open": true
      },
      "dialog": {
        "open": true
      }
    },
    "windows": [
      {
        "title": "N-Link",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
